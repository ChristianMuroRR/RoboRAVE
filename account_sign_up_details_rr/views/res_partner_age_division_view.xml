<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <record id="view_partner_age-division_tree" model="ir.ui.view">
            <field name="name">res.partner.age.division.tree</field>
            <field name="model">res.partner.age.division</field>
            <field name="arch" type="xml">
                <tree string="Partner Age Division" editable="bottom">
                    <field name="name" />
                    <field name="age_from" />
                    <field name="age_to" />
                </tree>
            </field>
        </record>
        <record id="view_partner_age_division_form" model="ir.ui.view">
            <field name="name">res.partner.age.division.form</field>
            <field name="model">res.partner.age.division</field>
            <field name="arch" type="xml">
                <form string="Partner Age Division">
                    <sheet>
                        <group col="4">
                            <field name="name" />
                            <field name="age_from" />
                            <field name="age_to" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_partner_age_division" model="ir.actions.act_window">
            <field name="name">Age Division</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner.age.division</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help">
                Manage the contact age division you want to have available in your system.
                For eg: 0-10, 11-20
            </field>
        </record>
        <menuitem id="menu_partner_age_division" action="action_partner_age_division" name="Age Division" sequence="2" parent="contacts.res_partner_menu_config"/>
</odoo>
